@model HerramientasProgFinal.Models.Doctor

@{
    ViewData["Title"] = "Details";
}

<body>
    <h1>Detalles</h1>

    <div>
        <h4>Doctor</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Nombre
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Nombre)
            </dd>
            <dt class="col-sm-2">
                Edad
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Edad)
            </dd>
            <dt class="col-sm-2">
                Especialidad
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Especialidad)
            </dd>
            <dt class="col-sm-2">
                Consultorio_
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Consultorio_.Nombre)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" class="btn btn-primary btn-sm" asp-route-id="@Model?.Id">Editar</a>
        <a asp-action="Index" class="btn btn-secondary btn-sm">Volver</a>
    </div>
</body>
</html>